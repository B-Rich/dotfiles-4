[ui]
username = Seth House <seth@eseth.com>
merge = vimdiff
ignore = ~/.hgignore

[defaults]
log = --limit 5 --template 'r{rev}\t{node|short}\t{date|age} ago\t{author|person}\n\t- {files}\n\t{desc|strip|fill68|tabindent}\n\n'

[web]
allow_archive = bz2 gz
port = 8001

[extensions]
color =
hgext.graphlog  =
hgext.purge =
hgext.record =
hgext.mq =
