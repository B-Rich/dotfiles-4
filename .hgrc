[ui]
username = Seth House <seth@eseth.com>
merge = vimdiff
ignore = ~/.hgignore

[alias]
llog = log --limit 5 --template '{rev}:{node|short}\t{date|age} ago\t{author|person}\n\t- {files}\n\t{desc|strip|fill68|tabindent}\n\n'

[web]
allow_archive = bz2 gz
port = 8001

[extensions]
color =
alias =
hgext.graphlog  =
hgext.purge =
hgext.record =
hgext.rebase = 
hgext.bookmarks =
