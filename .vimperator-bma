" Quick Search Keyward Bookmarks
bmark -t "Google Search" -k "g" http://www.google.com/search?q=%s
bmark -t "Google Feeling Lucky Search" -k "goto" http://www.google.com/search?q=%s&btnI=I%27m+Feeling+Lucky
bmark -t "Gmail Search" -k "mail" https://mail.google.com/mail/?search=query&q=%s
bmark -t "Google Site Search" -k "site" javascript:location=(%22http://www.google.com/search?q=site:%22+location.hostname+%22%20%22+%22%s%22)
bmark -t "Netflix Search" -k "netflix" http://www.netflix.com/Search?v1=%s
bmark -t "Wikipedia Search" -k "wiki" http://en.wikipedia.org/wiki/Special:Search?search=%s&go=Go
bmark -t "Dictionary.com Search" -k "dict" http://dictionary.reference.com/search?q=%s
bmark -t "Thesaurus.com Search" -k "thes" http://thesaurus.reference.com/search?q=%s
bmark -t "Nethack Search" -k "nethack" http://nethack.wikia.com/wiki/Special:Search?search=%s&go=1
