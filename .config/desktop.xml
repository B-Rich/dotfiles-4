<!-- Load this file via: gconftool-2 --load ~/src/dotfiles/.conf/desktop.xml -->
<!-- Also run: gsettings set org.gnome.desktop.interface gtk-key-theme "Emacs" -->
<gconfentryfile>
  <entrylist base="/desktop">

    <entry>
      <key>gnome/interface/gtk_key_theme</key>
      <schema_key>/schemas/desktop/gnome/interface/gtk_key_theme</schema_key>
      <value>
        <string>Emacs</string>
      </value>
    </entry>

    <entry>
      <key>gnome/keybindings/custom0/action</key>
      <value>
        <string>sh -c &apos;ZSHRUN=1 uxterm -geometry 90x5+0+0 +ls -bg white -fg black -T zshrun&apos;</string>
      </value>
    </entry>
    <entry>
      <key>gnome/keybindings/custom0/binding</key>
      <value>
        <string>&lt;Alt&gt;space</string>
      </value>
    </entry>
    <entry>
      <key>gnome/keybindings/custom0/name</key>
      <value>
        <string>zshrun</string>
      </value>
    </entry>

    <entry>
      <key>ibus/general/hotkey/trigger</key>
      <schema_key>/schemas/desktop/ibus/general/hotkey/trigger</schema_key>
      <value>
        <list type="string">
            <value>
              <string>Control+space</string>
            </value>
        </list>
      </value>
    </entry>
  </entrylist>

  <entrylist base="/apps">
    <entry>
      <key>metacity/global_keybindings/switch_to_workspace_1</key>
      <schema_key>/schemas/apps/metacity/global_keybindings/switch_to_workspace_1</schema_key>
      <value>
        <string>&lt;Alt&gt;F1</string>
      </value>
    </entry>
    <entry>
      <key>metacity/global_keybindings/switch_to_workspace_2</key>
      <schema_key>/schemas/apps/metacity/global_keybindings/switch_to_workspace_2</schema_key>
      <value>
        <string>&lt;Alt&gt;F2</string>
      </value>
    </entry>
    <entry>
      <key>metacity/global_keybindings/switch_to_workspace_3</key>
      <schema_key>/schemas/apps/metacity/global_keybindings/switch_to_workspace_3</schema_key>
      <value>
        <string>&lt;Alt&gt;F3</string>
      </value>
    </entry>
    <entry>
      <key>metacity/global_keybindings/switch_to_workspace_4</key>
      <schema_key>/schemas/apps/metacity/global_keybindings/switch_to_workspace_4</schema_key>
      <value>
        <string>&lt;Alt&gt;F4</string>
      </value>
    </entry>

    <entry>
      <key>metacity/window_keybindings/close</key>
      <schema_key>/schemas/apps/metacity/window_keybindings/close</schema_key>
      <value>
        <string>&lt;Mod4&gt;q</string>
      </value>
    </entry>

    <entry>
      <key>metacity/window_keybindings/maximize_horizontally</key>
      <schema_key>/schemas/apps/metacity/window_keybindings/maximize_horizontally</schema_key>
      <value>
        <string>&lt;Shift&gt;&lt;Mod4&gt;underscore</string>
      </value>
    </entry>

    <entry>
      <key>metacity/window_keybindings/maximize_vertically</key>
      <schema_key>/schemas/apps/metacity/window_keybindings/maximize_vertically</schema_key>
      <value>
        <string>&lt;Shift&gt;&lt;Mod4&gt;bar</string>
      </value>
    </entry>

    <entry>
      <key>metacity/window_keybindings/minimize</key>
      <schema_key>/schemas/apps/metacity/window_keybindings/minimize</schema_key>
      <value>
        <string>&lt;Mod4&gt;c</string>
      </value>
    </entry>

    <entry>
      <key>metacity/window_keybindings/toggle_maximized</key>
      <schema_key>/schemas/apps/metacity/window_keybindings/toggle_maximized</schema_key>
      <value>
        <string>&lt;Mod4&gt;x</string>
      </value>
    </entry>

    <entry>
      <key>metacity/global_keybindings/panel_main_menu</key>
      <schema_key>/schemas/apps/metacity/global_keybindings/panel_main_menu</schema_key>
      <value>
        <string>&lt;Mod4&gt;space</string>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>
